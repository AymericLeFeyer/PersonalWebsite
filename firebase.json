{
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/youtube",
        "destination": "https://www.youtube.com/channel/UClCAe7FyrIwpkt9H56XRndA",
        "type": 301
      }
    ],
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
  }
}
